<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:cwp="http://www.carewebframework.org/schema/plugin" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		 http://www.springframework.org/schema/beans/spring-beans.xsd
		 http://www.carewebframework.org/schema/plugin
		 http://www.carewebframework.org/schema/plugin/plugin-extensions.xsd">

	<beans profile="root">

		<cwp:plugin id="vistaFamilyHistory" name="Family History"
			url="~./org/carewebframework/vista/ui/familyhistory/main.zul">
			<cwp:serialization>
			</cwp:serialization>
			<cwp:resource>
				<cwp:help-resource module="vistaFamilyHistorylHelp" />
			</cwp:resource>
		</cwp:plugin>

	</beans>

	<beans profile="desktop">

		<bean name="vistaFamilyHistoryController"
			class="org.carewebframework.vista.ui.familyhistory.MainController" scope="prototype" />

	</beans>
</beans>